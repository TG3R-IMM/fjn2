	<script>
	
	document.addEventListener('DOMContentLoaded', () => {
	// Function to load translations from a JSON file
	async function loadTranslations() {
	try {
	const response = await fetch('/assets/translations.json');
	const translations = await response.json();
	
	// Extract the language code from the URL hash
	let lang = 'en'; // default language
	const supportedLanguages = Object.keys(translations); // ['en', 'pt', 'fr', 'ru', ...]
	
	// Get the language code from the hash, stripping the '#' symbol
	const hashLang = window.location.hash.substring(1);
	if (supportedLanguages.includes(hashLang)) {
	lang = hashLang;
	}
	
	// Check if translations for the selected language exist
	if (!translations[lang]) {
	console.warn(`No translations found for language: ${lang}`);
	return;
	}
	const languageData = translations[lang];
	
	// Apply translations
	const titleElement = document.querySelector('.zm-modal-body-title');
	if (titleElement && titleElement.innerText.trim() === 'Rename:') {
	titleElement.innerText = languageData.rename_title || titleElement.innerText;
	}
	
	const labelElement = document.querySelector('.dialog-rename-container__label');
	if (labelElement && labelElement.innerText.trim() === 'Name') {
	labelElement.innerText = languageData.name_label || labelElement.innerText;
	}
	
	document.querySelectorAll('.zm-btn').forEach(button => {
	if (button.innerText.trim() === 'Cancel') {
	button.innerText = languageData.cancel_button || button.innerText;
	}
	if (button.innerText.trim() === 'Change') {
	button.innerText = languageData.change_button || button.innerText;
	}
	});
	
	const participantsHeaderElement = document.querySelector('.participants-header__title');
	if (participantsHeaderElement && participantsHeaderElement.innerText.includes('Participants')) {
	participantsHeaderElement.innerHTML = participantsHeaderElement.innerHTML.replace('Participants', languageData.participants_title || 'Participants');
	}
	
	const footerLabelElement = document.querySelector('.footer-button-base__button-label');
	if (footerLabelElement && footerLabelElement.innerText.includes('Participants')) {
	footerLabelElement.innerHTML = footerLabelElement.innerHTML.replace('Participants', languageData.participants_title || 'Participants');
	}
	
	const moreButtonElement = document.querySelector('.dropdown .btn span');
	if (moreButtonElement && moreButtonElement.innerText.trim() === 'More') {
	moreButtonElement.innerText = languageData.more_button || moreButtonElement.innerText;
	}
	
	document.querySelectorAll('.zmu-portal-dropdown__menu-item-button').forEach(button => {
	if (button.innerText.trim() === 'Rename') {
	button.innerText = languageData.rename_button || button.innerText;
	}
	});
	
	// Add translation for the placeholder in the search box input
	const searchBoxInput = document.querySelector('.participants-search-box__input');
	if (searchBoxInput) {
	searchBoxInput.placeholder = languageData.find_participant || searchBoxInput.placeholder;
	}
	
	} catch (error) {
	console.error('Error loading translations:', error);
	}
	}
	
	// Run the function to load and apply translations
	loadTranslations();
	
	// Optional: Re-apply translations when the hash changes
	window.addEventListener('hashchange', loadTranslations);
	});
	
	async function fetchAPIData() {
	try {
	const response = await fetch('http://localhost:3900/state');
	const data = await response.json();
	console.log('API Data:', data);
	return data.attendeesById;
	} catch (error) {
	console.error('Error fetching participants:', error);
	}
	}
	
	async function populateParticipants() {
	const rawParticipants = await fetchAPIData();
	const participants = Object.values(rawParticipants);
	
	// Update the participant count
	document.querySelectorAll('.participantCount').forEach(element => {
	element.textContent = participants.length ? participants.length : 42;
	});
	
	// Function to sort participants by host/cohost and spotlighted status
	function sortParticipants(participants) {
	return participants.sort((a, b) => {
	// Prioritize spotlighted participants
	if (a.isSpotlighted && !b.isSpotlighted) return -1;
	if (!a.isSpotlighted && b.isSpotlighted) return 1;
	
	// If both are spotlighted or both are not, prioritize hosts
	if (a.isHost && !b.isHost) return -1;
	if (!a.isHost && b.isHost) return 1;
	
	// If neither are hosts or both are hosts, prioritize cohosts
	if (a.isCoHost && !b.isCoHost) return -1;
	if (!a.isCoHost && b.isCoHost) return 1;
	
	// If all conditions are equal, keep original order
	return 0;
	});
	}
	
	const sortedParticipants = sortParticipants(participants);
	
	// Update the first participant in the existing HTML structure
	function updateFirstParticipantHtml(participant) {
	document.getElementById('firstAvatar').src = participant.avatarUrl || '';
	document.getElementById('firstInitials').textContent = getInitials(participant.displayName);
	document.getElementById('newname').value = participant.displayName
	
	if (!participant.avatarUrl) {
	document.getElementById('firstAvatar').style.display = "none"
	document.getElementById('firstInitials').style.display = "block"
	}
	
	document.getElementById('firstDisplayName').textContent = participant.displayName;
	
	const micIcon = document.getElementById('firstMic');
	micIcon.innerHTML = participant.muted
	? `<svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="m4.573 9.306-.76.76A4.648 4.648 0 0 1 3.334 8a.5.5 0 1 1 1 0c0 .46.085.9.24 1.306Zm1.76-1.76-.953.953A2.681 2.681 0 0 1 5.333 8V3.334a2.667 2.667 0 0 1 5.331-.12l-.997.998v-.878a1.667 1.667 0 1 0-3.334 0v4.212Zm-.398 4.64.76-.758A3.667 3.667 0 0 0 11.667 8a.5.5 0 0 1 .999 0A4.667 4.667 0 0 1 8.5 12.64v1.693H10a.5.5 0 1 1 0 1H6a.5.5 0 0 1 0-1h1.5V12.64a4.637 4.637 0 0 1-1.565-.454Zm1.566-1.566A2.667 2.667 0 0 0 10.666 8v-.545l-1.09 1.092a1.671 1.671 0 0 1-1.03 1.028L7.501 10.62Z" fill="#DE2828"></path><path d="m15.5.5-15 15" stroke="#DE2828" stroke-linecap="round"></path></svg>`
	: `<svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="M9.667 8V3.333a1.667 1.667 0 0 0-3.334 0V8a1.667 1.667 0 0 0 3.334 0ZM8 .667a2.667 2.667 0 0 0-2.667 2.666V8a2.667 2.667 0 0 0 5.334 0V3.333A2.667 2.667 0 0 0 8 .667Zm.5 11.973A4.667 4.667 0 0 0 12.667 8a.5.5 0 1 0-1 0 3.667 3.667 0 1 1-7.334 0 .5.5 0 1 0-1 0A4.667 4.667 0 0 0 7.5 12.64v1.693H6a.5.5 0 1 0 0 1h4a.5.5 0 0 0 0-1H8.5V12.64Z" fill="#6E6E76"></path></svg>`;
	
	const videoIcon = document.getElementById('firstVideo');
	videoIcon.innerHTML = participant.isVideoOn
	? `<svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="M1.5 4H9a2 2 0 0 1 2 2v5.5a.5.5 0 0 1-.5.5H3a2 2 0 0 1-2-2V4.5a.5.5 0 0 1 .5-.5ZM12 10v1.5a1.5 1.5 0 0 1-1.5 1.5H3a3 3 0 0 1-3-3V4.5A1.5 1.5 0 0 1 1.5 3H9a3 3 0 0 1 3 3l2.8-2.1a.75.75 0 0 1 1.2.6v7a.75.75 0 0 1-1.2.6L12 10Zm0-1.25L15 11V5l-3 2.25v1.5Z" fill="#747487"></path></svg>`
	: `<svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="M10.486 3.393A2.986 2.986 0 0 0 9 3H1.5A1.5 1.5 0 0 0 0 4.5V10c0 1.053.543 1.98 1.364 2.515l.73-.731A2 2 0 0 1 1 10V4.5a.5.5 0 0 1 .5-.5H9c.26 0 .51.05.738.14l.748-.747ZM11 7.121l1.485-1.485L14.8 3.9a.75.75 0 0 1 1.2.6v7a.75.75 0 0 1-1.2.6L12 10v1.5a1.5 1.5 0 0 1-1.5 1.5H5.121l1-1H10.5a.5.5 0 0 0 .5-.5V7.121ZM15 11l-3-2.25v-1.5L15 5v6Z" fill="#E02828"></path><path d="m15.5.5-15 15" stroke="#DE2828" stroke-linecap="round"></path></svg>`;
	}
	
	updateFirstParticipantHtml(sortedParticipants[0]);
	
	// Function to create and insert participant elements
	function renderParticipants(participants) {
	const container = document.querySelector(".ReactVirtualized__Grid__innerScrollContainer");
	
	// Save the first .participants-item-position
	const firstElement = container.querySelector(".participants-item-position");
	
	// Clear the container's innerHTML
	container.innerHTML = '';
	
	// Add the saved firstElement back to the container
	if (firstElement) {
	container.appendChild(firstElement);
	}
	
	// Render the remaining participants, starting from the second one
	participants.forEach(participant => {
	const participantDiv = document.createElement("div");
	participantDiv.className = "participants-item-position";
	participantDiv.innerHTML = createParticipantHTML(participant);
	container.appendChild(participantDiv);
	});
	}
	
	function createParticipantHTML(participant) {
	const avatar = participant.avatarUrl
	? `<img src="${participant.avatarUrl}" alt="${participant.displayName}" style="width: 32px; height: 32px; border-radius: 8px; margin-right: 12px;">`
	: `<div class="participants-item__avatar" aria-hidden="true" style="font-size: 16px; width: 32px; height: 32px; line-height: 32px; text-align: center; color: rgb(255, 255, 255); flex: 0 0 auto; background-color: ${getRandomColor()};">${getInitials(participant.displayName)}</div>`;
	
	return `
	<div>
	<div role="application" id="participants-list-${participant.userId}" class="item-pos participants-li" tabindex="-1" aria-label="${participant.displayName}">
	<div class="participants-item__item-layout">
	<div class="participants-item__left-section">
	${avatar}
	<span class="participants-item__name-section">
	<span class="participants-item__display-name">${participant.displayName}</span>
	<span class="participants-item__name-label"></span>
	</span>
	</div>
	<div class="participants-item__right-section">
	<div class="participants-item__right-section--icons">
	${participant.muted
	? `<div class="participants-icon__icon-box"><svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="m4.573 9.306-.76.76A4.648 4.648 0 0 1 3.334 8a.5.5 0 1 1 1 0c0 .46.085.9.24 1.306Zm1.76-1.76-.953.953A2.681 2.681 0 0 1 5.333 8V3.334a2.667 2.667 0 0 1 5.331-.12l-.997.998v-.878a1.667 1.667 0 1 0-3.334 0v4.212Zm-.398 4.64.76-.758A3.667 3.667 0 0 0 11.667 8a.5.5 0 0 1 .999 0A4.667 4.667 0 0 1 8.5 12.64v1.693H10a.5.5 0 1 1 0 1H6a.5.5 0 0 1 0-1h1.5V12.64a4.637 4.637 0 0 1-1.565-.454Zm1.566-1.566A2.667 2.667 0 0 0 10.666 8v-.545l-1.09 1.092a1.671 1.671 0 0 1-1.03 1.028L7.501 10.62Z" fill="#DE2828"></path><path d="m15.5.5-15 15" stroke="#DE2828" stroke-linecap="round"></path></svg></div>`
	: `<div class="participants-icon__icon-box"><svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg" class="lazy-svg-icon__icon lazy-icon-icons/participants-list/audio-unmuted-1"><path fill-rule="evenodd" clip-rule="evenodd" d="M9.667 8V3.333a1.667 1.667 0 0 0-3.334 0V8a1.667 1.667 0 0 0 3.334 0ZM8 .667a2.667 2.667 0 0 0-2.667 2.666V8a2.667 2.667 0 0 0 5.334 0V3.333A2.667 2.667 0 0 0 8 .667Zm.5 11.973A4.667 4.667 0 0 0 12.667 8a.5.5 0 1 0-1 0 3.667 3.667 0 1 1-7.334 0 .5.5 0 1 0-1 0A4.667 4.667 0 0 0 7.5 12.64v1.693H6a.5.5 0 1 0 0 1h4a.5.5 0 0 0 0-1H8.5V12.64Z" fill="#6E6E76"></path></svg></div>`}
	${participant.isVideoOn
	? `<div class="participants-icon__icon-box"><svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="M1.5 4H9a2 2 0 0 1 2 2v5.5a.5.5 0 0 1-.5.5H3a2 2 0 0 1-2-2V4.5a.5.5 0 0 1 .5-.5ZM12 10v1.5a1.5 1.5 0 0 1-1.5 1.5H3a3 3 0 0 1-3-3V4.5A1.5 1.5 0 0 1 1.5 3H9a3 3 0 0 1 3 3l2.8-2.1a.75.75 0 0 1 1.2.6v7a.75.75 0 0 1-1.2.6L12 10Zm0-1.25L15 11V5l-3 2.25v1.5Z" fill="#747487"></path></svg></div>`
	: `<div class="participants-icon__icon-box"><svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="M10.486 3.393A2.986 2.986 0 0 0 9 3H1.5A1.5 1.5 0 0 0 0 4.5V10c0 1.053.543 1.98 1.364 2.515l.73-.731A2 2 0 0 1 1 10V4.5a.5.5 0 0 1 .5-.5H9c.26 0 .51.05.738.14l.748-.747ZM11 7.121l1.485-1.485L14.8 3.9a.75.75 0 0 1 1.2.6v7a.75.75 0 0 1-1.2.6L12 10v1.5a1.5 1.5 0 0 1-1.5 1.5H5.121l1-1H10.5a.5.5 0 0 0 .5-.5V7.121ZM15 11l-3-2.25v-1.5L15 5v6Z" fill="#E02828"></path><path d="m15.5.5-15 15" stroke="#DE2828" stroke-linecap="round"></path></svg></div>`}
	</div>
	</div>
	</div>
	</div>
	</div>
	`;
	}
	
	// Function to get initials from a name
	function getInitials(name) {
	const nameParts = name.trim().split(" ");
	if (nameParts.length > 1) {
	return nameParts[0][0] + nameParts[1][0];
	}
	return nameParts[0][0];
	}
	
	// Function to generate a random color for the avatar background
	function getRandomColor() {
	const colors = ["#f39c12", "#e74c3c", "#8e44ad", "#2980b9", "#27ae60", "#c0392b", "#d35400"];
	return colors[Math.floor(Math.random() * colors.length)];
	}
	
	// Render participants starting from the second one (index 1)
	renderParticipants(sortedParticipants.slice(1));
	}
	
	populateParticipants();
	
	const cursor = document.querySelector(".simulated-cursor");
	cursor.style.transform = `translate(500px, 900px)`;
	
	
	setTimeout(() => {
	cursor.style.transform = `translate(-500px, 490px)`;
	
	setTimeout(() => {
	document.querySelector('.simulated-cursor').classList.add('active')
	setTimeout(() => {
	document.querySelector('.simulated-cursor').classList.remove('active')
	}, 200)
	
	}, 2600)
	
	setTimeout(() => {
	document.getElementById('wc-container-right').style.right = "0"
	
	setTimeout(() => {
	cursor.style.transform = `translate(40px, 90px)`;
	}, 400)
	
	setTimeout(() => {
	cursor.style.transform = `translate(-120px, 90px)`;
	setTimeout(() => {
	document.getElementById('rightSection').style.display = "none"
	document.getElementById('rightSectionHovered').style.display = "block"
	}, 700)
	
	document.getElementById('participantsButton').style.bottom = "-100px"
	
	setTimeout(() => {
	cursor.style.transform = `translate(-40px, 90px)`;
	
	setTimeout(() => {
	document.querySelector('.simulated-cursor').classList.add('active')
	setTimeout(() => {
	document.querySelector('.simulated-cursor').classList.remove('active')
	}, 200)
	}, 1200)
	
	setTimeout(() => {
	document.getElementById('dropdown').style.display = "block"
	setTimeout(() => {
	cursor.style.transform = `translate(-80px, 120px)`;
	}, 500)
	
	
	setTimeout(() => {
	document.getElementById('dropdown').querySelector('.zmu-portal-dropdown__menu-item').classList.add("hovered")
	setTimeout(() => {
	document.querySelector('.simulated-cursor').classList.add('active')
	setTimeout(() => {
	document.querySelector('.simulated-cursor').classList.remove('active')
	}, 200)
	
	document.getElementById('dropdown').querySelector('.zmu-portal-dropdown__menu-item').classList.remove("hovered")
	document.getElementById('dropdown').querySelector('.zmu-portal-dropdown__menu-item').classList.add("active")
	setTimeout(() => {
	document.getElementById('wc-container-right').style.right = "-400px"
	cursor.style.transform = `translate(-550px, 390px)`;
	document.getElementById('dropdown').style.display = "none"
	document.getElementById('rightSection').style.display = "block"
	document.getElementById('rightSectionHovered').style.display = "none"
	
	document.getElementById('changeNameModal').style.display = "block"
	
	}, 300)
	}, 3300)
	}, 1200)
	}, 1500)
	}, 2000)
	
	}, 400)
	
	}, 3000)
	
	}, 1000)
	
	
	</script>
	
	
